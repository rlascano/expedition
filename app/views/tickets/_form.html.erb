<div class="form">
  <%= form_with(model: @ticket, local: true) do |form| %>

    <% if @ticket.errors.any? %>
      <div id="error_explanation">
        <h2>
          <%= pluralize(@ticket.errors.count, "error") %> prohibited
          this article from being saved:
        </h2>
        <ul>
          <% @ticket.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
        </ul>
      </div>
    <% end %>

    <%= form.collection_select(:client_id, Enterprise.all, :id, :name) %>
    <%= form.collection_select(:product_id, Product.all, :id, :name) %>
    <%= form.collection_select(:carrier_id, Enterprise.all, :id, :name) %>
    <%= form.collection_select(:trucker_id, Trucker.all, :id, :full_name) %>

    <%= form.submit %>

  <% end %>
</div>
